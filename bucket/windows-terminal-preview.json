        [
            "WindowsTerminal.exe",
            "Windows Terminal Preview"
        ]
    ],
    "checkver": {
        "url": "https://api.github.com/repos/microsoft/terminal/releases",
        "jsonpath": "$.[?(@.prerelease == true)]",
        "regex": "\"tag_name\": \"v([\\d.]+)\"[\\s\\S]+?(?<version>[\\d.]+)_x"
